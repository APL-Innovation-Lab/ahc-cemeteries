{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/address": "^1.8",
        "drupal/advagg": "^4.0",
        "drupal/asset_injector": "^2",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^6",
        "drupal/calendar": "^1.0@alpha",
        "drupal/calendar_view": "^1.0",
        "drupal/colorbox": "^1.7",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_perms": "^2",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/core-recommended": "^9.4",
        "drupal/csv_importer": "^1.11",
        "drupal/ctools": "^3.11",
        "drupal/date_popup": "^1.1",
        "drupal/datetimehideseconds": "^1.1",
        "drupal/dbug": "^2.0",
        "drupal/disable_messages": "^2.1",
        "drupal/draggableviews": "^2",
        "drupal/editor_advanced_link": "^2",
        "drupal/editoria11y": "^1.0",
        "drupal/endroid_qr_code": "^3",
        "drupal/entity_hierarchy": "^3.0",
        "drupal/entity_reference_tree": "^2.0",
        "drupal/entityform_block": "^1.0@alpha",
        "drupal/facets": "^2",
        "drupal/feeds": "3.x-dev",
        "drupal/feeds_ex": "^1.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "^3.0",
        "drupal/field_permissions": "^1.0",
        "drupal/fixed_block_content": "^1.1",
        "drupal/flood_control": "^2.2",
        "drupal/focal_point": "^2.0",
        "drupal/fullcalendar": "^1.0@alpha",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/gin": "^3.0@alpha",
        "drupal/honeypot": "^1.30",
        "drupal/iframe": "^2.12",
        "drupal/jquery_ui_resizable": "^1.0",
        "drupal/jsonapi_explorer": "^1.1",
        "drupal/jsonapi_extras": "^3.14",
        "drupal/jsonapi_permission": "^1.2",
        "drupal/jwt": "^2.0",
        "drupal/key": "^1.17",
        "drupal/key_auth": "^1.1",
        "drupal/ldap": "^4.0",
        "drupal/maillog": "^1.0@beta",
        "drupal/mailsystem": "^4.3",
        "drupal/material_base": "^1.8",
        "drupal/media_entity_download": "^2.1",
        "drupal/memcache": "^2.1",
        "drupal/menu_block": "^1.7",
        "drupal/metatag": "^1.13",
        "drupal/migrate_plus": "^6.0",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/office_hours": "^1.3",
        "drupal/pathauto": "^1.6",
        "drupal/publishcontent": "^1.4",
        "drupal/purge": "^3.0",
        "drupal/queue_mail": "^1.3",
        "drupal/redirect": "^1.5",
        "drupal/rules": "3.x-dev@dev",
        "drupal/rules_send_big_email": "^1.4",
        "drupal/rules_token": "^2.0",
        "drupal/scheduler": "^1.3",
        "drupal/schema_metatag": "^2.3",
        "drupal/search_api": "^1.16",
        "drupal/search_api_autocomplete": "1.x-dev@dev",
        "drupal/search_api_sorts": "^1.0",
        "drupal/security_review": "^1.0",
        "drupal/simple_styleguide": "^1.8",
        "drupal/single_datetime": "^1.7",
        "drupal/smart_date": "^3.3",
        "drupal/snippet_manager": "^1.8",
        "drupal/superfish": "^1.3",
        "drupal/theme_switcher": "^1.2",
        "drupal/token": "^1.6",
        "drupal/tour_ui": "^1.0@RC",
        "drupal/twig_tweak": "^2.5",
        "drupal/ultimate_cron": "^2",
        "drupal/uswds": "^2.4",
        "drupal/varnish_purge": "^2.1",
        "drupal/vefl": "^3.0",
        "drupal/video_embed_field": "^2.4",
        "drupal/view_mode_page": "^4",
        "drupal/views_accordion": "^2.0",
        "drupal/views_aggregator": "^2",
        "drupal/views_autocomplete_filters": "^1.3",
        "drupal/views_bulk_edit": "^2.4",
        "drupal/views_bulk_operations": "^4",
        "drupal/views_conditional": "1.5",
        "drupal/views_data_export": "^1.0@RC",
        "drupal/views_jump_menu": "^1.0@beta",
        "drupal/views_simple_math_field": "^3",
        "drupal/views_slideshow": "^4.6",
        "drupal/webform": "^6",
        "drupal/webform_content_creator": "^2",
        "drupal/webform_rest": "^4.0",
        "drupal/webform_views": "^5.0@alpha",
        "drush/drush": "^10",
        "endroid/qr-code-bundle": "^3.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "bin-dir": "/usr/local/bin",
        "allow-plugins": {
            "drupal/core-composer-scaffold": true,
            "endroid/installer": true,
            "composer/installers": true,
            "zodiacmedia/drupal-libraries-installer": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "html"
            }
        },
        "installer-paths": {
            "html/core": ["type:drupal-core"],
            "html/libraries/{$name}": ["type:drupal-library"],
            "html/modules/contrib/{$name}": ["type:drupal-module"],
            "html/profiles/contrib/{$name}": ["type:drupal-profile"],
            "html/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "html/modules/custom/{$name}": ["type:drupal-custom-module"],
            "html/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
